export class NavData{

  /**
   * 根据类型获取导航栏的类别
   * @param type 类别 电影 综艺 少儿 体育 纪录片....
   * @returns Array<String> 类型的分类
   */
  public static  navList(type:String):Array<String>{
      switch (type){
        case "电影":{
          return ["全部","动作","戏剧","科幻","恐怖","战争","剧情","爱情","喜剧","记录","犯罪","R级","悬疑","警匪","动画","网络电影","儿童","经典","青春","古装"]
        }
        case "电视剧":{
          return ["喜剧","古装","家庭","犯罪","动作","奇幻","武侠","爱情","悬疑","科幻","警匪","谍战","商战","偶像","军事","战争","都市","青春偶像剧","剧情","历史","经典","情景","网剧","其他"]
        }
        case "综艺":{
          return ["脱口秀","真人秀","搞笑","访谈","生活","晚会","美食","游戏","亲子","旅游","文化","体育","时尚","经济","益智","演出","歌舞","音乐","播报","其他"]
        }
        case "动漫":{
          return ["情感","科幻","热血","推理","搞笑","神魔","真人","冒险","剧情","历史","萝莉","校园","动作","激战","运动","战争","少年","少女","社会","原创","亲子","益智","励志","格斗","恋爱","美少女","LOLI","竞技","童话","教育","其他"]
        }
        case "记录":{
          return ["人物","军事","历史","自然","探险","科技","文化","刑侦","社会","旅游","其他"]
        }
        case "短剧":{
          return null;
        }
        case "少儿":{
          return ["儿歌","益智","手工","绘画","玩具","搞笑","英语","早教","动画","教学","国学","交通工具","科幻","真人","探索","其他"]
        }
        default :{
          return null;
        }
      }
  }

  /**
   * 获取国家分类的导航条
   * @returns Array<String> 国家的分类
   */
  public static country():Array<String>{
    return ["全部","大陆","香港","台湾","美国","韩国","日本","法国","英国","德国","泰国","印度","其他"]
  }


  /**
   * 获取国家分类的导航条
   * @returns Array<String> 国家的分类
   */
  public static videoTime():Array<String>{
    return ["全部","2024","2023","2022","2021","2020","2019","2018","2017","2016","2015","2014"]
  }



}

